<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.fvideo" version="6.1.2" name="FVideo" provider-name="Choehn">
<requires>
  <import addon="xbmc.python" version="3.0.0"/>
  <import addon="script.module.requests" version="2.22.0" />
  <import addon="script.module.simplejson" version="3.16.1" />
  <import addon="script.module.beautifulsoup4" version="4.9.3+matrix.1"/>
</requires>
<extension point="xbmc.python.pluginsource" library="main.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">Xem video tu Fshare su dung TK VIP</summary>
  <description lang="en_GB">Thong tin lay tu Google, thuvienhd.com, bien tap</description>
  <disclaimer lang="en_GB">Noi dung Video</disclaimer>
  <assets>
    <icon>icon.png</icon>
    <fanart>fanart.jpg</fanart>
  </assets>
  <news>Can co tai khoan VIP Fshare</news>
</extension>
</addon>

<addon id="repository.vinaxem" name="VinaXEM Repo" version="6.0.0" provider-name="Choehn">
