        <dir>
            <info compressed="false">https://raw.githubusercontent.com/taikhoanonlinevn/repository.vinaxem/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/taikhoanonlinevn/repository.vinaxem/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/taikhoanonlinevn/repository.vinaxem/master/zips/</datadir>
        </dir>
